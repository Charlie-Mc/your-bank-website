<html lang="en">
{{>headPartial.hbs}}
<body>

{{#if accounts}}
    {{#each accounts}}
    <div class="account">
        <h2>{{this.name}}</h2>
        <p>{{this.id}}</p>
        <p>{{this.balance}}</p>
        <p>{{this.currency}}</p>
        <p>{{this.accountType}}</p>
    </div>
    {{/each}}
{{/if}}
{{#if account}}
