        <h2>{{this.id}}</h2>
        <p>{{this.name}}</p>
        <p>{{this.balance}}</p>
        <p>{{this.currency}}</p>
        <p>{{this.accountType}}</p>
    </div>
{{/if}}
{{#if json}}
    {{json}}
{{/if}}
</body>
</html>